<?xml version="1.0" encoding="UTF-8"?>
<!-- This file defines attributes of standard OPGEE classes / XML elements, including
     name, short description, unit (if any) and deterministic default values. It is used
     to support parsing the XML model definition and to used by the Smart Default system
     to fill in unspecified attributes. It can also eventually support a GUI, e.g., in
     creating pulldown menus of options for categorical parameters and radio buttons for
     binaryÂ ones.
     -->
<AttrDefs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="attributes.xsd">
  <ClassAttrs name="Model">
    <!-- Maximum number of iterations for process loops -->
    <AttrDef name="maximum_iterations" type="int">10</AttrDef>

    <!-- Change threshold for iteration loops. Requires a Process to set a change variable. -->
    <AttrDef name="maximum_change" type="float">0.001</AttrDef>
  </ClassAttrs>

  <ClassAttrs name="Stream">
    <!-- the zero values aren't real defaults; they just avoid having None as a default -->
    <AttrDef name="temperature" type="float" unit="degF">0</AttrDef>
    <AttrDef name="pressure" type="float" unit="psia">0</AttrDef>
  </ClassAttrs>

  <ClassAttrs name="Analysis">
    <Options name="GWP horizon" default="100">
      <Option label="20 year">20</Option>
      <Option label="100 year">100</Option>
    </Options>
    <Options name="GWP version" default="AR5">
      <Option>AR4</Option>
      <Option>AR5</Option>
      <Option label="AR5 with C-cycle feedback">AR5_CCF</Option>
    </Options>
    <Options name="Functional unit" default="oil">
      <Option label="1 MJ Crude oil">oil</Option>
      <Option label="1 MJ Natural gas">gas</Option>
    </Options>
    <Options name="Energy basis" default="LHV">
      <Option label="Lower heating value">LHV</Option>
      <Option label="Higher heating value">HHV</Option>
    </Options>

    <AttrDef name="GWP_horizon" options="GWP horizon" type="int" unit="years"/>
    <AttrDef name="GWP_version" options="GWP version" type="str"/>
    <AttrDef name="functional_unit" options="Functional unit" type="str"/>
    <AttrDef name="energy_basis" options="Energy basis" type="str"/>
  </ClassAttrs>

  <!-- Attributes shared by all Processes. These values can be overridden in subprocesses -->
  <ClassAttrs name="Process">
    <AttrDef name="leak_rate" type="float" desc="Fraction of gas streams leaked to environment" unit="fraction">0.002</AttrDef>
  </ClassAttrs>

  <ClassAttrs name="Field">
    <!-- Production methods -->
    <AttrDef name="downhole_pump" type="binary" desc="Downhole pump">1</AttrDef>
    <AttrDef name="water_reinjection" type="binary" desc="Water reinjection">1</AttrDef>
    <AttrDef name="natural_gas_reinjection" type="binary" desc="Natural gas reinjection">1</AttrDef>
    <AttrDef name="water_flooding" type="binary" desc="Water flooding">0</AttrDef>
    <AttrDef name="gas_lifting" type="binary" desc="Gas lifting">0</AttrDef>

    <!-- mutually exclusive -->
    <AttrDef name="gas_flooding" type="binary" desc="Gas flooding" exclusive="flooding">0</AttrDef>
    <AttrDef name="steam_flooding" type="binary" desc="Steam flooding" exclusive="flooding">0</AttrDef>

    <Options name="oil_sands_mine_type" default="None">
      <Option>None</Option>
      <Option>Integrated with upgrader</Option>
      <Option>Non-integrated with upgrader</Option>
    </Options>
    <AttrDef name="oil_sands_mine" options="oil_sands_mine_type" desc="Oil sands mine"/>


    <!-- synchronized - these are just for testing purposes -->
    <AttrDef name="sync_attr_1" type="binary" synchronized="sync_test">0</AttrDef>
    <AttrDef name="sync_attr_2" type="binary" synchronized="sync_test">0</AttrDef>

    <!-- Field properties -->
    <AttrDef name="country" desc="Field location (country)" type="str">Generic</AttrDef>
    <AttrDef name="name" desc="Field name" type="str">Generic</AttrDef>
    <AttrDef name="age" unit="yr" desc="Field age" type="float" GT="0" LT="150">38</AttrDef>
    <AttrDef name="depth" unit="ft" desc="Field depth" type="float" GT="0" LT="25000">7240.0</AttrDef>
    <AttrDef name="oil_prod" unit="bbl_oil/d" desc="Oil production volume" type="float" GT="0">2098.0</AttrDef>
    <AttrDef name="num_prod_wells" desc="Number of producing wells" type="int" GT="0">24</AttrDef>
    <AttrDef name="num_water_inj_wells" desc="Number of water injecting wells" type="int" GE="0">20</AttrDef>
    <AttrDef name="well_diam" unit="in" desc="Well diameter" type="float" GT="0">2.78</AttrDef>
    <AttrDef name="prod_index" unit="bbl_oil/(psia*day)" type="float" desc="Productivity index">17.0</AttrDef>
    <AttrDef name="res_press" unit="psia" desc="Average reservoir pressure" type="float">1556.6</AttrDef>
    <AttrDef name="res_temp" unit="degF" desc="Average reservoir temperature" type="float">200.0</AttrDef>
    <AttrDef name="res_thickness" unit="ft" desc="Average reservoir thickness" type="float">50</AttrDef>
    <AttrDef name="res_perm" unit="mD" desc="Reservoir permeability" type="float">100</AttrDef>
    <AttrDef name="wellhead_pressure" type="float" unit="psia" >500</AttrDef>
    <AttrDef name="wellhead_temperature" type="float" unit="degF" >80</AttrDef>
    <AttrDef name="offshore" type="binary" desc="Offshore?">0</AttrDef>
    <!-- Fluid properties -->
    <AttrDef name="API" desc="API gravity of produced crude" unit="degAPI" type="float" has_smart_default="true">
      <Requires>GOR</Requires>
    </AttrDef>
    <AttrDef name="API_bitumen" unit="degAPI" type="float" desc="API gravity of bitumen">8.0</AttrDef>
    <AttrDef name="total_dissolved_solids" desc="water total dissolved solid (TDS)" unit="mg/L" type="float">5000.0</AttrDef>
    <AttrDef name="gas_comp_N2" unit="mol_pct" type="float">2.86</AttrDef>
    <AttrDef name="gas_comp_CO2" unit="mol_pct" type="float">0.33</AttrDef>
    <AttrDef name="gas_comp_C1" unit="mol_pct" type="float">89.18</AttrDef>
    <AttrDef name="gas_comp_C2" unit="mol_pct" type="float">5.30</AttrDef>
    <AttrDef name="gas_comp_C3" unit="mol_pct" type="float">1.62</AttrDef>
    <AttrDef name="gas_comp_C4" unit="mol_pct" type="float">0.71</AttrDef>
    <AttrDef name="gas_comp_H2S" unit="mol_pct" type="float">0.0</AttrDef>    <!-- TODO: needs a default value -->
    <!-- Production practices -->
    <Options name="Flood gases" default="NG">
      <Option desc="Natural gas">NG</Option>
      <Option desc="Nitrogen">N2</Option>
      <Option desc="Carbon dioxide">CO2</Option>
    </Options>
    <Options name="Liquids unloading" default="None">
      <Option>Plunger</Option>
      <Option>No plunger</Option>
      <Option>None</Option>
    </Options>
    <Options name="source_CO2" default="reservoir">
      <Option desc="natural subsurface reservoir">Reservoir</Option>
      <Option>Anthropogenic</Option>
    </Options>
    <AttrDef name="GOR" desc="Gas-to-oil ratio" unit="scf/bbl_oil" type="float">2429.3</AttrDef>

    <AttrDef name="WOR" desc="Water-to-oil ratio" unit="bbl_water/bbl_oil" type="float">6.0</AttrDef>
    <AttrDef name="WOR-MEAN" desc="WOR mean (deps. on age)" unit="" type="float">6.0</AttrDef>
    <AttrDef name="WOR-SD" desc="WOR std deviation (deps. on age)" unit="" type="float">1.4</AttrDef>

    <AttrDef name="WIR" desc="Water injection ratio" unit="bbl_water/bbl" type="float">7.0</AttrDef>
    <AttrDef name="GLIR" desc="Gas lifting injection ratio" unit="scf/bbl_liquid" type="float">364.0</AttrDef>
    <AttrDef name="GFIR" desc="Gas flooding injection ratio" unit="scf/bbl_oil" type="float">3644.0</AttrDef>
    <AttrDef name="flood_gas_type" desc="Flood gas" options="Flood gases" type="str"/>
    <AttrDef name="liquids_unloading" options="Liquids unloading" type="str"/>
    <AttrDef name="perc_CO2_breakthrough" desc="Fraction of CO2 breaking through to producers" unit="percent" type="float">59</AttrDef>
    <AttrDef name="source_CO2" desc="" options="source_CO2" type="str"/>
    <AttrDef name="perc_sequestration_credit" desc="Percentage of sequestration credit assigned to the oilfield" unit="percent" type="float">0</AttrDef>
    <AttrDef name="SOR" desc="Steam-to-oil ratio" unit="bbl_steam/bbl_oil" type="float">0</AttrDef>
    <AttrDef name="fraction_elec_onsite" desc="Fraction of required electricity generated onsite" type="float">0.0</AttrDef>
    <AttrDef name="fraction_remaining_gas_inj" desc="Fraction of remaining gas reinjected" type="float">0.5</AttrDef>
    <AttrDef name="fraction_water_reinjected" desc="Fraction of water produced that is reinjected" type="float">1.0</AttrDef>
    <AttrDef name="fraction_steam_cogen" desc="Fraction of steam generation via co-generation" type="float">0.0</AttrDef>
    <AttrDef name="fraction_steam_solar" desc="Fraction of steam generation via solar thermal" type="float">0.0</AttrDef>
    <AttrDef name="pressure_outlet" type="float" unit="psia" desc="Separator pressure (final if multi-stage)">100.0</AttrDef>
    <AttrDef name="temperature_outlet" type="float" unit="degF" desc="Separator temperature (final if multi-stage)">90.0</AttrDef>
    <AttrDef name="gas_pressure_after_boosting" type="float" unit="psia" desc="Separator gas outlet pressure after boosting compressors">500</AttrDef>
    <AttrDef name="eta_compressor" type="float" unit="percent" desc="Compressor efficiency">75.0</AttrDef>
    <!-- Processing practices -->
    <Options name="upgrader_type" default="None">
      <Option>None</Option>
      <Option>Delayed coking</Option>
      <Option>Hydroconversion</Option>
      <Option desc="Combined hydroconversion and fluid coking">Combined</Option>
    </Options>
    <Options name="crude_oil_dewatering_output" default="CrudeOilStabilization">
      <Option desc="Heavy oil upgrading">HeavyOilUpgrading</Option>
      <Option desc="Heavy oil dilution">HeavyOilDilution</Option>
      <Option desc="Crude oil stabilization">CrudeOilStabilization</Option>
    </Options>
    <Options name="gas_processing_path" default="Acid wet gas">
      <Option>None</Option>
      <Option desc="Dehydrator">Minimal</Option>
      <Option desc="Dehydrator + Amine Process">Acid Gas</Option>
      <Option desc="Dehydrator + Demethanizer">Wet Gas</Option>
      <Option desc="Dehydrator + Amine Process + Demethanizer">Acid Wet Gas</Option>
      <Option desc="Dehydration then direct injection of sour CO2-rich stream">Sour gas reinjection</Option>
      <Option desc="Dehydrator + Compressor + Membrane + Amine Process + Demethanizer">CO2-EOR Membrane</Option>
      <Option desc="Dehydrator + Ryan Holmes Process (Demethanizer Included)">CO2-EOR Ryan Holmes</Option>
    </Options>
    <AttrDef name="heater_treater" type="binary" desc="Heater/treater">0</AttrDef>
    <AttrDef name="steam_quality_at_generator_outlet" unit="frac" type="float" desc="steam quality at generator outlet">0.7</AttrDef>
    <AttrDef name="steam_quality_after_blowdown" unit="frac" type="float" desc="desired steam quality after blowdown and recycle (if any)">0.7</AttrDef>
    <AttrDef name="crude_oil_dewatering_output" options="crude_oil_dewatering_output" desc="process receiving crude oil from dewatering"/>
    <AttrDef name="upgrader_type" desc="Upgrader type" options="upgrader_type" type="str"/>
    <AttrDef name="gas_processing_path" desc="Associated gas processing path" options="gas_processing_path" type="str"/>
    <AttrDef name="FOR" unit="scf/bbl_oil" desc="Ratio of flaring to oil production" type="float">141.3</AttrDef>
    <AttrDef name="VOR" unit="scf/bbl_oil" desc="Ratio of venting to oil production" type="float">5.0</AttrDef>
    <AttrDef name="fraction_diluent" desc="Volume fraction of diluent in diluted crude" type="float">0.0</AttrDef>
    <AttrDef name="temperature_mined_bitumen" desc="Mined bitumen temperature" type="float" unit="degF">80.0</AttrDef>
    <AttrDef name="pressure_mined_bitumen" desc="Mined bitumen pressure" type="float" unit="psia">14.70</AttrDef>
    <!-- LandUse impacts -->
    <Options name="ecosystem_C_richness" default="Moderate">
      <Option desc="Low carbon richness (semi-arid grasslands)">Low</Option>
      <Option desc="Moderate carbon richness (mixed)">Moderate</Option>
      <Option desc="High carbon richness (forested)">High</Option>
    </Options>
    <Options name="field_development_intensity" default="Moderate">
      <Option desc="Low intensity development and low oxidation">Low</Option>
      <Option desc="Moderate intensity development and moderate oxidation">Moderate</Option>
      <Option desc="High intensity development and high oxidation">High</Option>
    </Options>
    <AttrDef name="ecosystem_richness" options="ecosystem_C_richness" desc="Ecosystem carbon richness" type="str"/>
    <AttrDef name="field_development_intensity" options="field_development_intensity" desc="Field development intensity" type="str"/>
    <!-- name="Transport_and_storage -->
    <AttrDef name="Fraction_oil_transport_tanker" desc="Fraction  of oil transported by ocean tanker" type="float">1</AttrDef>
    <AttrDef name="Fraction_oil_transport_barge" desc="Fraction of oil transported by barge" type="float">0</AttrDef>
    <AttrDef name="Fraction_oil_transport_pipeline" desc="Fraction of oil transported by pipeline" type="float">1</AttrDef>
    <AttrDef name="Fraction_oil_transport_rail" desc="Fraction of oil transported by rail" type="float">0</AttrDef>
    <AttrDef name="Fraction_oil_transport_truck" desc="Fraction of oil transported by truck" type="float">0</AttrDef>
    <AttrDef name="Oil_transport_distance_tanker" unit="mi" desc="Ocean tanker transport distance (one way)" type="float">8000</AttrDef>
    <AttrDef name="Oil_transport_distance_barge" unit="mi" desc="Barge transport distance (one way)" type="float">500</AttrDef>
    <AttrDef name="Oil_transport_distance_pipeline" unit="mi" desc="Pipeline transport distance (one way)" type="float">1000</AttrDef>
    <AttrDef name="Oil_transport_distance_rail" unit="mi" desc="Rail transport distance (one way)" type="float">800</AttrDef>
    <AttrDef name="Oil_transport_distance_truck" unit="mi" desc="Truck transport distance (one way)" type="float">100</AttrDef>
    <AttrDef name="ocean_tanker_size" unit="tons" desc="Ocean tanker size, if applicable" type="float">250000</AttrDef>
    <AttrDef name="small_sources_adder" unit="gCO2eq/MJ" desc="Small source emissions adder" type="float">0.5</AttrDef>

    <AttrDef name="barge_capacity" unit="tonne" type="float" desc="Barge capacity">22500.0</AttrDef>
    <AttrDef name="ocean_tanker_speed" unit="mile/hr" type="float" desc="Ocean tanker speed">18.52</AttrDef>
    <AttrDef name="barge_speed" unit="mile/hr" type="float" desc="Barge speed">5.0</AttrDef>
    <AttrDef name="ocean_tanker_load_factor_dest" unit="frac" type="float" desc="Ocean tanker load factor (origin to destination)">0.8</AttrDef>
    <AttrDef name="barge_load_factor_dest" unit="frac" type="float" desc="Barge load factor (origin to destination)">0.8</AttrDef>
    <AttrDef name="ocean_tanker_load_factor_origin" unit="frac" type="float" desc="Ocean tanker load factor (destination to origin)">0.7</AttrDef>
    <AttrDef name="barge_load_factor_origin" unit="frac" type="float" desc="Barge load factor (destination to origin)">0.6</AttrDef>
    <AttrDef name="energy_intensity_rail_transport" unit="btu/tonne/mile" type="float" desc="Energy intensity of rail transport">370.0</AttrDef>
    <AttrDef name="energy_intensity_truck" unit="btu/tonne/mile" type="float">969.0</AttrDef>
    <AttrDef name="energy_intensity_pipeline_turbine" unit="btu/tonne/mile" type="float" desc="Energy intensity of pipeline transport">240.0</AttrDef>
    <AttrDef name="energy_intensity_pipeline_engine_current" unit="btu/tonne/mile" type="float" desc="Reciprocating enginer powered (current)">270.0</AttrDef>
    <AttrDef name="energy_intensity_pipeline_engine_future" unit="btu/tonne/mile" type="float" desc="Reciprocating engine powered (future)">260.0</AttrDef>
    <AttrDef name="frac_power_pipeline_turbine" unit="frac" type="float" desc="Fraction of power generation technologies for pipeline pumping and compression stations">0.55</AttrDef>
    <AttrDef name="frac_power_pipeline_engine_current" unit="frac" type="float" desc="Reciprocating enginer powered (current)">0.36</AttrDef>
