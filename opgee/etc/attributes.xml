    <AttrDef name="losses_feed" unit="btu/mmBtu" type="float" desc="Feed losses: btu/mmbtu fuel transported">62.0</AttrDef>

    <AttrDef name="fraction_diluent_transport_tanker" unit="frac" type="float">1.0</AttrDef>
    <AttrDef name="fraction_diluent_transport_barge" unit="frac" type="float">0.0</AttrDef>
